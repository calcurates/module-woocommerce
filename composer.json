{
    "name": "calcurates/module-woocomerce",
    "description": "Calcurates Integration for WooCommerce",
    "keywords": ["shipping"],
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "psr/log": "^1.1"
    },
    "authors": [
        {
            "name": "Calcurates",
            "homepage": "https://calcurates.com"
        }
    ],
    "type": "wordpress-plugin",
    "version": "1.0.0",
    "config": {
        "vendor-dir": "lib",
        "optimize-autoloader": true,
        "platform": {
            "php": "7.1.3"
        }
    },
    "autoload": {
		"psr-4": {
			"Calcurates\\": "src/"
		}
	},
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "szepeviktor/phpstan-wordpress": "^0.7",
        "php-stubs/woocommerce-stubs": "^5.3",
        "friendsofphp/php-cs-fixer": "^3.0"
    }
}
